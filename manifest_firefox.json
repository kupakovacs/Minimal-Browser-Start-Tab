{
  "manifest_version": 2,
  "name": "Random Image New Tab",
  "version": "1.0",
  "description": "A surprisingly good-looking new tab extension that pulls a random image from Wikipedia (any language).",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "random-image-newtab@example.com",
      "strict_min_version": "57.0"
    }
  }
}
